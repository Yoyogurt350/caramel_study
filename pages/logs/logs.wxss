.logs{
    display: flex;
    flex-wrap: wrap;
    width: 750rpx;
    height: 320rpx;
    margin-top: 5rpx;
    border:10rpx solid #eee;
}
.sum-item{
    width: 350rpx;
    height: 160rpx;
    display: flex;
    flex-direction: column;
    text-align: center;
    margin: 5rpx 0;
}
.sum-item .sum-title{
    width: 350rpx;
    margin: 10rpx 0;
    color:#666;

}
.sum-item .sum-text{
    color: #E78075;
}
.detail{
    width: 750rpx;
    font-size: 40rpx;
    display: flex;
}
.detail-title{
    flex: 1;
}
.detail .detail-type{
    flex: 1;
    text-align: right;
    font-size: 35rpx;
    padding: 10rpx;
}
.detail .detail-type text{
    padding: 10rpx;
}
.active{
    color:#E78075;
}
.list{
    padding: 5rpx;
    width: 750rpx;
}
.list-item{
    padding: 5rpx;
    margin: 5rpx auto;
    display: flex;
    border-bottom: 1rpx solid #ccc;
    justify-content: space-between;
}

